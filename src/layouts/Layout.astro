---
import { SEO } from "astro-seo";
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<SEO 
		title="Komornik Płock - Sylwester Karolak"
		description="Profesjonalne usługi zapewni doświadczony komornik w Płocku - Sylwester Karolak. Kompleksowy zakres usług zarówno dla dłużników, jak i wierzycieli."
		canonical="https://komornikkarolak.pl"
		/>
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<!-- <title>{title}</title> -->
	</head>
	<body>
		<slot />
	</body>
</html>
