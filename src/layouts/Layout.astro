---
import { SEO } from "astro-seo";
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!DOCTYPE html>
<html lang="pl">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<SEO
			title="Komornik Sądowy Płock - Sylwester Karolak"
			description="Komornik Sądowy przy Sadzie Rejonowym w Płocku Sylwester Karolak, kancelaria komornicza nr XIII, 09-402 Płock, ul. Strzelecka 7/14, tel. 881-158-093"
			canonical="https://komornikkarolak.pl"
		/>
		<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
		<link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
		<link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
		<link rel="manifest" href="/site.webmanifest" />
		<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5" />
		<meta name="msapplication-TileColor" content="#2b5797" />
		<meta name="theme-color" content="#ffffff" />
		<meta name="generator" content={Astro.generator} />
		<meta name="keywords" content="komornik płock, sylwester karolak, komornik sądowy płock, kancelaria komornicza płock" />
		<script async defer type="text/partytown" src="https://analytics.umami.is/script.js" data-website-id="66421acc-98ae-4c19-9faf-d091d02b529c"></script>
	</head>
	<body>
		<slot />
	</body>
</html>
